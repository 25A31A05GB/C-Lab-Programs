#include <iostream>
using namespace std;

int main()
{
    int n, key, count = 0;

    cout << "Enter size of array: ";
    cin >> n;

    int arr[100];

    cout << "Enter elements:\n";
    for(int i = 0; i < n; i++)
        cin >> arr[i];

    cout << "Enter key element: ";
    cin >> key;

    for(int i = 0; i < n; i++)
    {
        if(arr[i] == key)
            count++;
    }

    cout << "Number of occurrences: " << count;

    return 0;
}
